{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "> 1%",
            "Explorer >= 10",
            "Edge >= 12",
            "Chrome >= 49",
            "Firefox >= 55",
            "Safari >= 9.1",
            "iOS >= 9",
            "Android >= 4.4"
          ]
        }
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ]
}
